<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="attachableCombine" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">

    <MR>true</MR>

    <!--
        base weight = 20412kg

    -->

    <mrCombine unloadingPower="25" threshingIdlePower="25" threshingPowerFx="0.7" irlSpotTonPerHour="300" maxFieldThreshingSpeed="10" />

    <parentFile xmlFilename="$data/vehicles/amityTechnology/hrv12R/hrv12R.xml">
        <attributes>

            <set path="vehicle.base.components.component(0)#centerOfMass" value="0 1.2 0.25"/>
            <set path="vehicle.base.components.component(0)#mass" value="19600"/>

            <set path="vehicle.wheels.wheelConfigurations.wheelConfiguration(0).wheels.wheel(0).physics#damper" value="4"/>
            <set path="vehicle.wheels.wheelConfigurations.wheelConfiguration(0).wheels.wheel(0).physics#spring" value="100"/>

            <set path="vehicle.wheels.wheelConfigurations.wheelConfiguration(0).wheels.wheel(1).physics#damper" value="4"/>
            <set path="vehicle.wheels.wheelConfigurations.wheelConfiguration(0).wheels.wheel(1).physics#spring" value="100"/>


            <set path="vehicle.powerConsumer#ptoRpm" value="540" />
            <set path="vehicle.powerConsumer#neededMinPtoPower" value="0" />
            <set path="vehicle.powerConsumer#neededMaxPtoPower" value="0" />
            <set path="vehicle.powerConsumer#maxForce" value="20" />
            <set path="vehicle.powerConsumer#forceNode" value="hrv12R_main_component1" />


            <!-- no brake -->
            <remove path="vehicle.attachable.brakeForce" />

            <set path="vehicle.dischargeable.dischargeNode(0)#emptySpeed" value="120"/>
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit(0)#mrCenterOfMass" value="0 3 -0.75"/>


        </attributes>
    </parentFile>

</vehicle>